:sass
  .region_header
    padding: 3px
  .ui_region
    background: rgb(245, 241, 226)
    padding: 0 0 10px
    margin: 0 10px
  .over
    background: rgb(234, 227, 197)
    cursor: hand
    cursor: pointer
  .Help
    color: #222
    font-size: 16px
    overflow: hidden
    min-width: 900px
    .HelpNav
      background: rgb(245, 241, 226)
      border: 1px solid rgb(234, 227, 197)
      margin: 0 0 12px 0
      padding: 5px 0
      p
        margin: 0
        padding: 0
      a
        color: #000
        line-height: 1.8
        margin: 0 -4px 0 1px
        padding: 9px 10px
        text-decoration: none
      a:hover
        color: #000
        background: #fff
    .Extras
      float: right
      margin-top: 40px
      padding-bottom: 10px
      width: 38%
      .Docs, .Regions
        background: rgb(245, 241, 226)
        border-bottom: 2px solid rgb(234, 227, 197)
        margin: 0
        *
          margin: 0 10px 4px 10px
        * *
          margin: 0
        h2
          background: rgb(234, 227, 197)
          color: #000
          font-weight: normal
          margin: 0 0 5px 0
          padding: 2px 5px
        .over
          background: rgb(234, 227, 197)
      p, li
        color: #333
        line-height: 1.3
        margin: 0 10px 10px 0
    .MainTopics
      float: left
      margin: 0 0 0 8px
      width: 60%
      min-height: 300px
      h1
        margin-bottom: 12px
      h2
        border-bottom: 4px solid #444
        font-weight: normal
        font-size: 1.4em
        margin: 24px 0 6px 0
        padding: 2px 2px 6px 0
      h3
        color: #222
        font-size: 1.1em
        margin: 6px 0 6px 0
      h4
        color: #222
        font-size: 0.95em
        margin: 3px 0 3px 0
        padding: 3px 0
      p, li
        color: #444
        line-height: 1.4
        margin: 0 0 6px 10px
      ul, ol
        margin: 0 0 0 20px
      ul
        margin: 0 0 0 20px
        list-style: square
      ol
        margin: 0 0 0 28px
        list-style-type: decimal
      code
        background: #eee
        color: #222
        line-height: inherit
        padding: 4px 2px
      pre
        background: #eee
        color: #222
        line-height: 1.4
        margin: 16px 0 16px 0
        padding: 12px 0 12px 12px
        width: 99%
        white-space: pre-wrap
        word-wrap: break-word
  @media print
    #header, #footer, .HelpNav, .Extras, hr
      display: none
    #footer
      display: none
    .MainTopics
      min-width: 90%
    body
      background:-color white
      font-family: "Cambria", "Hoefler Text", "Georgia", "Times New Roman", times, serif
      font-size: 12pt
    a:link:after, a:visited:after
      color: #222
      content: " : " attr(href) ""
      font-size: 9pt
      font-weight: normal
  div.popup
    border: none
    .reference
      border: none
      width: 50em
      height: 100%